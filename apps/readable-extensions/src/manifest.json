{
  "name": "Readable",
  "description": "Read and share anything readable in Readable!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "readable_logo.png"
  }
}
